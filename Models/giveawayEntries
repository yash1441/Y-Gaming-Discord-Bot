module.exports = (sequelize, DataTypes) => {
	return sequelize.define('giveawayEntries', {
        message_id: {
            type: DataTypes.STRING,
            allowNull: false,
        },
        channel_id: {
            type: DataTypes.STRING,
            allowNull: false,
        },
        server_id: {
            type: DataTypes.STRING,
            allowNull: false,
        },
        discord_id: {
            type: DataTypes.STRING,
            allowNull: false,
        },
        entries: {
            type: DataTypes.INTEGER,
            defaultValue: 1,
            allowNull: false,
        },
        won: {
            type: DataTypes.BOOLEAN,
            allowNull: false,
        },
	}, {
		timestamps: true,
	});
};